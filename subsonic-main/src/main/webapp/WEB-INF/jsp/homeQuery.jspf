<form method="post" action="home.view" name="searchForm" onsubmit="return submitForm(this);">
	<input type="hidden" name="listType" value="${model.listType}"/>
	<input type="hidden" name="listGroup" value="${model.listGroup}"/>
	<input type="hidden" name="listUsers" value="${model.listUsers}"/>
	<table><tr>
		<td><input type="text" name="query" id="query" size="50"></td>
		<td><a href="#" onclick="return submitForm(this)"><i class="fa fa-search"></i></a></td>
	</tr></table>
</form>
