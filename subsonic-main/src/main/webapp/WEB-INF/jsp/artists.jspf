<div class="row">
  
	<c:forEach items="${model.artists}" var="artistRecommendation" varStatus="loopStatus">
	<div class="col-sm-6 col-md-6 col-lg-3">
		<div>
		      <sub:url value="artist.view" var="artistUrl">
		          <sub:param name="id" value="${artistRecommendation.artistUri}" />
		      </sub:url>
			  <a href="${artistUrl}">
					<img src="${artistRecommendation.imageUrl}" class="img-thumbnail">
				</a>
				<div class="detail caption no-overflow">
            <a href="#" onclick="return onPlayTopTracks('${artistRecommendation.artistUri}', 'P');">
                      <img src="<spring:theme code="playImage"/>" alt="Play top tracks" title="Play top tracks">
            </a>
					  <a href="artist.view?id=${artistRecommendation.artistUri}">
						${artistRecommendation.artistName}
						</a>
				</div>
		</div>
  </div>
	</c:forEach>
	  
</div>
